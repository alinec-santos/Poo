<AnchorPane id="AnchorPane" prefHeight="289.0" prefWidth="508.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TelaTemasController">
