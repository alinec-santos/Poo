            <HBox prefHeight="60.0" prefWidth="508.0" />
            <Button fx:id="btnIniciar" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleactionbtnIniciar" text="Iniciar">
               <VBox.margin>
                  <Insets left="200.0" />
